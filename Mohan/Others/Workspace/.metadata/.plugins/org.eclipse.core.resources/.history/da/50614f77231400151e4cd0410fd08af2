package com.hackerRank.java.dataStructure;

public class Java_1D_Array {

	public static void main(String args[])
	{
		int[] _arr={0,0,0,0,0};
		int m=1;
		
		for(int i=0;i<_arr.length;)
		{
			if(_arr[i+1]==0)
			{
				i+=1;
			}
			else if(_arr[i+m]==0)
			{
				i+=m;
			}
			else if(_arr[i-1]==0)
			{
				_arr[i]=9;
				i-=1;
			}
			else
			{
				i++;
			}
		}
		if(i>_arr.length)
		{
			
		}
	}
}
