<?xml version="1.0"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
   version="2.0">
	

	<managed-bean>
		<managed-bean-name>GlobalCommonNotesDataBean</managed-bean-name>
		<managed-bean-class>
			com.acs.enterprise.common.program.contactmanagement.view.bean.GlobalCommonNotesDataBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		
		<managed-property>
			<property-name>entityNotesSearchCriteriaVO</property-name>
			<value>#{entityNotesSearchCriteriaVO}</value>
		</managed-property>
		
	</managed-bean>

	<managed-bean>
		<managed-bean-name>GlobalCommonNotesControllerBean</managed-bean-name>
		<managed-bean-class>
			com.acs.enterprise.common.program.contactmanagement.view.bean.GlobalCommonNotesControllerBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>


    <managed-bean>
		<managed-bean-name>entityNotesSearchCriteriaVO</managed-bean-name>
		<managed-bean-class>
			com.acs.enterprise.common.program.contactmanagement.common.vo.EntityNotesSearchCriteriaVO
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>


	<application>
		


		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
		<message-bundle>com.acs.enterprise.common.program.contactmanagement.view.nl.PGM_IN_GlobalCommonNotes</message-bundle>
		<locale-config>
			<default-locale>en</default-locale>
		</locale-config>
	</application>
</faces-config>