<?xml version="1.0"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
   version="2.0">
	
		
	<managed-bean>
		<managed-bean-name>globalCaseSearchControllerBean</managed-bean-name>
		<managed-bean-class>
			com.acs.enterprise.common.program.contactmanagement.view.bean.GlobalCaseSearchControllerBean
		</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>globalCaseSearchDataBean</managed-bean-name>
		<managed-bean-class>
			com.acs.enterprise.common.program.contactmanagement.view.bean.GlobalCaseSearchDataBean
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
	  		<property-name>caseRecordSearchCriteriaVO</property-name>
	  		<value>#{caseRecordSearchCriteriaVO}</value>
	  	</managed-property>
	</managed-bean>

		<managed-bean>
		<managed-bean-name>
			caseRecordSearchCriteriaVO
		</managed-bean-name>
		<managed-bean-class>
			com.acs.enterprise.common.program.contactmanagement.common.vo.CaseRecordSearchCriteriaVO
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>	
</managed-bean>
</faces-config>
