	<navigation-rule>
		<from-view-id>
			/jsp/LogCase/LogCase.jsp
		</from-view-id>
		<navigation-case>
			<from-action>#{logCaseControllerBean.renderAddAttachmentPage}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>
				/jsp/LogCase/inc_logCaseAddAttachments.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>
			/jsp/LogCase/inc_logCaseAddAttachments.jsp
		</from-view-id>
		<navigation-case>
			<from-action>#{logCaseControllerBean.cancelAttachments}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>
				/jsp/LogCase/LogCase.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>
